try:
    num1 = 10
    num2 = 0
    print(num1/num2)
except ZeroDivisionError:
    print("Cannot divide by zero")
finally:
    print("But will try in a different way")

    try:
        num3 = 10
        num4 = 0
        print(num3/num4)
    except Exception as error:
        print(f"The error is--------->  {error}")
